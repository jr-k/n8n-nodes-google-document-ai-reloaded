{
  "name": "n8n-nodes-google-document-ai-reloaded",
  "version": "1.3.3",
  "description": "n8n community node for Google Document AI",
  "keywords": [
    "n8n-community-node-package",
    "google",
    "document ai",
    "ocr",
    "document processing"
  ],
  "license": "MIT",
  "homepage": "https://github.com/jr-k/n8n-nodes-google-document-ai-reloaded",
  "author": {
    "name": "Ephraim M",
    "email": "ephraim@example.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jr-k/n8n-nodes-google-document-ai-reloaded.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist",
    "index.js"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/GoogleServiceAccountApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/GoogleDocumentAI/GoogleDocumentAi.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "@google-cloud/vision": "^4.3.2"
  }
}
